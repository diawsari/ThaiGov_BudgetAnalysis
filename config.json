{
    "dir_path": "__YOUR_DIR__",
    "file_pattern": "(\\d+)_Budget_red_stripe.csv",
    "sca_cross" : false,
    "fy" : 2024,
    "group_by": "BUDGETARY_UNIT",
    "data_columns": {
        "amount": "AMOUNT",
        "year": "BUDGET_YEAR",
        "obliged": "OBLIGED",
        "ministry": "BUDGETARY_UNIT"
    },
    "obliged_year": 2024,
    "plot": {
        "title": "Budget Distribution by Ministry",
        "labels": {
            "budget_year": "Year",
            "amount": "Amount (Millions)"
        },
        "direction": "top",
        "n_entries": 10,
        "color": "MINISTRY"
    },
    "logging": {
        "level": "INFO",
        "format": "%(asctime)s - %(levelname)s - %(message)s"
    }
}

